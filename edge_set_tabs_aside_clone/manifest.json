{
    "manifest_version": 2,
    "name": "edge_set_tabs_aside_clone",
    "version": "1.0",

    "background": {
        "scripts": ["background.js"]
    },

    "browser_action": {
        "browser_style": true,
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon-16.png",
            "32": "icon-32.png"
        }
    },

    "permissions": ["<all_urls>", "tabs", "bookmarks"]
}